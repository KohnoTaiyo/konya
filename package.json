{
  "name": "konya",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write './app/**/*.{js,jsx,ts,tsx}'",
    "lint:precommit": "eslint 'app/**/*.{ts,tsx}'",
    "fmt:precommit": "prettier --write './app/**/*.{js,jsx,ts,tsx,json,css,scss}'",
    "prepare": "husky install",
    "db": "prisma format && prisma db push",
    "studio": "prisma studio"
  },
  "lint-staged": {
    "app/**/*.{ts,tsx}": "yarn run lint:precommit",
    "app/**/*.{js,jsx,ts,tsx,json,css,scss}": "yarn run fmt:precommit"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.445.0",
    "@aws-sdk/s3-request-presigner": "^3.445.0",
    "@prisma/client": "5.5.2",
    "browser-image-compression": "^2.0.2",
    "next": "14.0.0",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.48.2",
    "tabler-icons-react": "^1.56.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.0.0-beta.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "autoprefixer": "^10",
    "classcat": "^5.0.4",
    "eslint": "^8",
    "eslint-config-next": "14.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "postcss": "^8",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "prisma": "^5.5.2",
    "tailwindcss": "^3",
    "typescript": "^5"
  }
