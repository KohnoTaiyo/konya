{
  "name": "konya",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write './app/**/*.{js,jsx,ts,tsx}'",
    "lint:precommit": "eslint 'app/**/*.{ts,tsx}'",
    "fmt:precommit": "prettier --write './app/**/*.{js,jsx,ts,tsx,json,css,scss}'",
    "prepare": "husky install"
  },
  "lint-staged": {
    "app/**/*.{ts,tsx}": "yarn run lint:precommit",
    "app/**/*.{js,jsx,ts,tsx,json,css,scss}": "yarn run fmt:precommit"
  },
  "dependencies": {
    "next": "14.0.0",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "autoprefixer": "^10",
    "classcat": "^5.0.4",
    "eslint": "^8",
    "eslint-config-next": "14.0.0",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "postcss": "^8",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "tailwindcss": "^3",
    "typescript": "^5"
  }
}
